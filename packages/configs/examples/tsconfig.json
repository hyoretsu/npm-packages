{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@app/*": ["app/*"],
			"@assets/*": ["assets/*"],
			"@components/*": ["components/*"],
			"@context/*": ["context/*"],
			"@hooks/*": ["hooks/*"],
			"@public/*": ["../public/*"],
			"@services/*": ["services/*"],
			"@utils/*": ["utils/*"]
		}
	},
	"exclude": ["node_modules"],
	"extends": ["@hyoretsu/configs/ts-frontend"],
	"include": ["./*.js", "./*.cjs", "./*.mjs", "./*.ts"]
}
